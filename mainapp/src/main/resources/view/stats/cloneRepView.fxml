<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="138.0" prefWidth="342.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.ams.repstats.controllers.stats.CloneRepViewController">
    <children>
        <TextField fx:id="tbURL" layoutX="15.0" layoutY="52.0" prefHeight="25.0" prefWidth="304.0"
                   text="https://github.com/" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"
                   AnchorPane.topAnchor="52.0"/>
        <ProgressBar fx:id="pbDownload" layoutX="90.0" layoutY="98.0" prefHeight="25.0" prefWidth="161.0" progress="0.0"
                     AnchorPane.bottomAnchor="12.0" AnchorPane.leftAnchor="90.0" AnchorPane.rightAnchor="91.0">
            <padding>
                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
            </padding>
        </ProgressBar>
        <Button fx:id="btExit" layoutX="256.0" layoutY="106.0" mnemonicParsing="false" onAction="#exitButtonAction"
                prefHeight="25.0" prefWidth="70.0" text="Закрыть" AnchorPane.bottomAnchor="14.0"
                AnchorPane.rightAnchor="14.0"/>
        <Label layoutX="99.0" layoutY="14.0" text="Введите URL репозитория" AnchorPane.leftAnchor="99.0"
               AnchorPane.rightAnchor="99.0" AnchorPane.topAnchor="28.0"/>
        <Button fx:id="btChoose" layoutX="14.0" layoutY="106.0" mnemonicParsing="false" onAction="#chooseButtonAction"
                prefHeight="25.0" text="Загрузить" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0"/>
    </children>
</AnchorPane>
